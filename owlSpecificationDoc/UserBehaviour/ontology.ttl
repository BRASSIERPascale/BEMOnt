                                  rdfs:subClassOf <http://hit2gap.eu/h2g#ZoneOccupancy> ;
                                  owl:disjointWith <http://hit2gap.eu/h2g#ZoneOccupied> ;
                                  rdfs:comment "Determines the corrresponding building space have nobody inside (empty room)"@en ;
                                  rdfs:label "Zone Empty"@en .


###  http://hit2gap.eu/h2g#ZoneOccupancy
<http://hit2gap.eu/h2g#ZoneOccupancy> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://hit2gap.eu/h2g#BuildingSimpleState> ;
                                      rdfs:comment "Determines the number of people that are in a specific building space. In an abstract way, it determines wether the building space is empty or occupied."@en ;
                                      rdfs:label "Zone Occupancy"@en .


###  http://hit2gap.eu/h2g#ZoneOccupied
<http://hit2gap.eu/h2g#ZoneOccupied> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://hit2gap.eu/h2g#ZoneOccupancy> ;
                                     rdfs:comment "Determines the corresponding building space have someone inside. Thus, the specific building space have one or more people inside."@en ;
                                     rdfs:label "Zone Occupied"@en .


