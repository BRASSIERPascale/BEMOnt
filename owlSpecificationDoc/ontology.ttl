@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://hit2gap.eu/h2g#> .

<http://hit2gap.eu/h2g#> rdf:type owl:Ontology ;
                          <http://purl.org/dc/elements/1.1/contributor> "Gulben Calis (gulbencalis@gmail.com)"@en ,
                                                                        "Richard Chbeir (richard.chbeir@univ-pau.fr)"@en ;
                          <http://purl.org/dc/elements/1.1/creator> "Aitor Corchero (aitor.corchero@eurecat.org)"@en ,
                                                                    "Pierre Bourreau (pbourreau@nobatek.com)"@en ,
                                                                    "Yudith Cardinale (yudith.cardinale@gmail.com)"@en ;
                          <http://purl.org/dc/elements/1.1/description> "This semantic model is focused on describing the occupants behaviour inside a building. For that, the proposed semantic model is based on the idetification of Drivers Needs and Actions for each occupant to make underestandable the occupant-building interaction and different comfort states."@en ;
                          <http://purl.org/dc/elements/1.1/title> "Hit2Gap Ontology-User Behavior"@en ;
                          <http://purl.org/dc/terms/license> "https://opensource.org/licenses/MIT"@en ;
                          owl:versionInfo "This current version includes the initial version of the semantic model based on the diagrams generated."@en .

