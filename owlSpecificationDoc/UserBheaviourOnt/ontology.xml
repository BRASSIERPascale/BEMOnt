        <rdfs:subClassOf rdf:resource="http://hit2gap.eu/h2g#Driver"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://hit2gap.eu/h2g#isBasedOn"/>
                        <owl:someValuesFrom rdf:resource="http://hit2gap.eu/h2g#OccupantBehaviour"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://hit2gap.eu/h2g#isBasedOn"/>
                        <owl:someValuesFrom rdf:resource="http://hit2gap.eu/h2g#TimeBehaviour"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


