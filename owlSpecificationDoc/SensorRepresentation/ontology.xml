    <!-- http://www.opengis.net/gml/_Feature -->


    <owl:Class rdf:about="http://www.opengis.net/gml/_Feature">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hit2gap.eu/h2g/h2gSensor#locatedInHemisphere"/>
