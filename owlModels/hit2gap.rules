@prefix h2g_bi: <http://hit2gap.eu/h2g/h2gBI#> .
@prefix services: <http://hit2gap.eu/h2g/Services#> .
@prefix ssn: <http://www.w3.org/ns/ssn/#> .

[ruleContains: (h2g_bi:contains owl:inverseOf h2g_bi:isContainedIn) (?s h2g_bi:contains ?o) -> (?o h2g_bi:isContainedIn ?s)]
[ruleIsLocatedIn: (?s h2g_bi:isLocatedIn ?o) (?o h2g_bi:isContainedIn ?p) -> (?s h2g_bi:isLocatedIn ?p)]
[ruleMadeBySensor: (ssn:madeObservation owl:inverseOf ssn:madeBySensor) (?s ssn:madeObservation ?o) -> (?o ssn:madeBySensor ?s)]
[ruleConnectedTo: (?s  ssn:madeObservation ?o) (?o ssn:hasFeatureOfInterest ?f) -> (?s h2g_bi:isConnectedTo ?f)]
[ruleConnects: (h2g_bi:isConnectedTo owl:inverseOf h2g_bi:connects) (?s h2g_bi:isConnectedTo ?o) -> (?o h2g_bi:connects ?s)]
[ruleLocated:  (?s h2g_bi:isConnectedTo ?f) (?f h2g_bi:isLocatedIn ?o) -> (?s h2g_bi:isLocatedIn ?o)]
[ruleEqPartOfService1: (services:partOfService owl:inverseOf services:usesModel) (?service services:usesModel ?model) -> (?model services:partOfService ?service)]
[ruleEqPartOfService2: (services:partOfService owl:inverseOf services:usesModel) (?model services:partOfService ?service) -> (?service services:usesModel ?model)]
[ruleEqOutputs1: (services:hasOutput owl:inverseOf services:model) (?model services:hasOutput ?output) -> (?output services:model ?model)]
[ruleEqOutputs2: (services:hasOutput owl:inverseOf services:model) (?output services:model ?model) -> (?model services:hasOutput ?output)]
[ruleTransServiceOutput: (?output services:model ?model) (?model services:partOfService ?service) -> (?output services:service ?service)]
