    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:isDefinedBy"/>
        <IRI>http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#hasUnit</IRI>
